{
  "name": "sync-branches",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "test": "npm run lint && ava --no-cache",
    "test:watch": "ava --watch",
    "test:coverage": "nyc ava",
    "lint": "eslint index.js src test"
  },
  "keywords": [],
  "author": "Marko Gresak <marko@gresak.io> (https://gresak.io/)",
  "license": "MIT",
  "engines": {
    "node": ">= 6.9.x"
  },
  "dependencies": {
    "@octokit/rest": "^15.1.7",
    "dotenv": "^4.0.0",
    "git-url-parse": "^8.1.0",
    "githubhook": "^1.7.1",
    "lodash": "^4.17.3",
    "nodegit": "^0.16.0",
    "nodemailer": "^6.4.16",
    "nodemailer-mailgun-transport": "^1.2.4",
    "npid": "^0.4.0",
    "rimraf": "^2.6.1",
    "semver-sort": "0.0.4",
    "yamljs": "^0.2.8"
  },
  "devDependencies": {
    "ava": "^0.17.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "mock-require": "^2.0.0",
    "mockdate": "^2.0.1",
    "nyc": "^10.0.0",
    "sinon": "^1.17.6"
  },
  "ava": {
    "files": [
      "test/**/*.js",
      "!test/mocks/**.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-runtime"
    ],
    "require": [
      "babel-core/register"
    ],
    "ignore": "test.js",
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
